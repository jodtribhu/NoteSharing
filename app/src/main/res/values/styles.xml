<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="AppTheme.ActionBar.Transparent" parent="AppTheme">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="colorPrimaryDark">#34373A</item>

        <item name="android:navigationBarColor">#34373A</item>
        <item name="colorPrimary">@android:color/transparent</item>
        <item name="android:textColor">#121111</item>
        <item name="android:titleTextStyle">@style/AppTheme.ActionBar.Transparent.TitleTextStyle</item>
    </style>
    <style name="AppTheme.ActionBar.Transparent.TitleTextStyle"
        parent="TextAppearance.AppCompat.Widget.ActionBar.Title">
        <item name="android:textColor">#00121111</item>
    </style>
</resources>